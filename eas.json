{
  "cli": {
    "version": ">= 15.0.10",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "IS_DEV": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "preview:simulator": {
      "extends": "preview",
      "ios": {
        "simulator": true
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
